@article{kang_accurate_2020,
	title = {Accurate detection of lameness in dairy cattle with computer vision: {A} new and individualized detection strategy based on the analysis of the supporting phase},
	volume = {103},
	issn = {0022-0302},
	shorttitle = {Accurate detection of lameness in dairy cattle with computer vision},
