	journal = {Journal of Dairy Science},
	author = {Kang, X. and Zhang, X. D. and Liu, G.},
	month = nov,
	year = {2020},
	keywords = {deep learning, dairy cattle, lameness detection, computer vision technology, supporting phase, \_3\_lameness},
