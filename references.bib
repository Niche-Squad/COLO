	keywords = {machine learning, \_2\_lasso, \_2\_ridge-regression, \_6\_cross-validation, \_4\_milk, \_7\_classification, \_2\_logistic-regression, \_1\_hyparam, \_4\_lying-time, \_4\_lying-bouts, \_2\_naive-bayes, heat stress, shade, sprinklers, \_3\_heat-stress, \_4\_temperature, \_4\_humidity, \_4\_respiration-rate, \_8\_precision, \_8\_accuracy, \_8\_f1, \_8\_recall, \_0\_appeal},
	pages = {501--524},
