	author = {Brand, W. and Wells, A. T. and Smith, S. L. and Denholm, S. J. and Wall, E. and Coffey, M. P.},
	month = apr,
	year = {2021},
	keywords = {deep learning, transfer learning, \_0\_appeal, genetic algorithms, pregnancy status},
